<div class="container">
    <div class="section-title">
      <h2>Update</h2>
      <p>News Slider</p>
    </div>  
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>
  
      <!-- Wrapper for slides -->
      <div class="carousel-inner">

        <div class="item active">
          <img src="<%=newsData[0].urlToImage%>" alt="Los Angeles"  style="width:100%; height: 650px;">
          <div class="carousel-caption">
            <h3 style=" background: rgba(229, 232, 229, 0.364);"><%=newsData[0].title%></h3>
            <!-- <p>LA is always so much fun!</p> -->
          </div>
        </div>
  
        <div class="item">
          <img src="<%=newsData[1].urlToImage%>" alt="Chicago" style="width:100%; height: 650px;">
          <div class="carousel-caption">
            <h3 style=" background: rgba(229, 232, 229, 0.364);"><%=newsData[1].title%></h3>
            <!-- <p>LA is always so much fun!</p> -->
          </div>
        </div>
      
        <div class="item">
          <img src="<%=newsData[2].urlToImage%>" alt="New york" style="width:100%; height: 650px;">
          <div class="carousel-caption">
            <h3 style=" background: rgba(229, 232, 229, 0.364);"><%=newsData[2].title%></h3>
            <!-- <p>LA is always so much fun!</p> -->
          </div>
        </div>
      </div>
  
      <!-- Left and right controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div><br> 